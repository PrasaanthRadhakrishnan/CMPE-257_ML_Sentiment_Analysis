{
  "manifest_version" : 2,
  "name" : "Extension Launcher",
  "description" : "Extension for Sentiment Analysis",
  "version" : "1.0.0",
  "browser_action" :{
    "default_popup" : "sample_extension.html"
  },
  "background": {
    "scripts": ["sample_extension.js"]
  },
  "content_security_policy" : "script-src 'self' https://unpkg.com/ionicons@5.5.2/dist/ionicons/ionicons.esm.js 'self'",
  "permissions" : ["tabs"]
}
